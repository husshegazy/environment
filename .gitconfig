	email = husshegazydev@gmail.com
	name = Huss Hegazy
[pull]
	ff = only
[rerere]
	enabled = true
[alias]
	change-commits = "!f() { VAR=$1; OLD=$2; NEW=$3; shift 3; git filter-branch --env-filter \"if [[ \\\"$`echo $VAR`\\\" = '$OLD' ]]; then export $VAR='$NEW'; fi\" $@; }; f"
[protocol "file"]
	allow = always
[pager]
	diff = false
[diff]
  external = difft